<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Elements Colors-->
    <SolidColorBrush x:Key="PrimaryColor" Color="#232528"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#2F3136"/>
    <LinearGradientBrush x:Key="AccentColor" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Offset="0" Color="#0097ff"/>
        <GradientStop Offset="1" Color="#0058ff"/>
    </LinearGradientBrush>
    
    <!--Text Colors-->
    <SolidColorBrush x:Key="TextActiveColor" Color="#e9e9e9"/>
    <SolidColorBrush x:Key="TextPassiveColor" Color="#5d5d5d"/>

</ResourceDictionary>